        <QueryParameters>
          <QueryParameter Name="@システム部署">
            <Value>=Parameters!システム部署.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--
-- システム部受注残　2012/03/01::2013/04/25 購入依頼計追加::2014/06仕切予定日追加：
-- 2014/7/1：：複数DB採用、仕切予定日。
-- 2016/12::注文確定から商品区分を追加、2017/01::行重複発生：原因：行照合を追加で正常化。
-- システム受注残::購入依頼
-- and shirei.注文種別 &lt; '3'  --1:注文  2:購入  3:出庫 
-- 

with 社内注文テーブル1
AS (SELECT
org.注文NO AS 注文NO1,max(org.注文枝番) AS 注文枝番1
FROM
tbl社内注文テーブル AS org
WHERE
org.注文書作成日 &gt;= '2010/09/01'
group by org.注文NO) 


SELECT  distinct tbl社内注文テーブル.注文NO
,tbl社内注文テーブル.注文枝番
,tbl社内注文テーブル.客先名
,tbl社内注文テーブル.希望納品日
,tbl社内注文テーブル.依頼先部署コード
,tbl社内注文明細テーブル.行番
,tbl社内注文明細テーブル.商品コード
,tbl社内注文明細テーブル.商品名
,tbl社内注文明細テーブル.規格仕様
,tbl社内注文明細テーブル.予定数量
,tbl社内注文明細テーブル.予定単価
,tbl社内注文明細テーブル.予定金額
,kk.生産仕切単価
,tbl社内注文明細テーブル.見積NO
,tbl社内注文テーブル.注文書作成日
,tbl社内注文テーブル.生産部受付日
,tbl案件テーブル.担当者名
,tbl案件テーブル.グループ記号
,tbl社内注文テーブル.仕切日
,tbl社内注文明細テーブル.仕切日 as 明細仕切日
,mst担当者マスタ.担当者名 as 生産担当者
,tbl社内注文テーブル.注文承認
,tbl社内注文テーブル.依頼状況
,tbl社内注文テーブル.変更区分 
,'手配合計'=case 
              when tbl社内注文明細テーブル.行番=1 then konyu.iraikei 
              else 0 end 
,prj_seisan.dbo.tbl注文書確定明細.仕切予定日
,prj_seisan.dbo.tbl注文書確定明細.商品区分 
,'出庫依頼額'=case 
              when tbl社内注文明細テーブル.行番=1 then shutu.出庫依頼額 
              else 0 end 

FROM    社内注文テーブル1

INNER JOIN tbl社内注文テーブル ON 
           社内注文テーブル1.注文NO1 = tbl社内注文テーブル.注文NO AND 
           社内注文テーブル1.注文枝番1 = tbl社内注文テーブル.注文枝番
INNER JOIN tbl社内注文明細テーブル ON 
           社内注文テーブル1.注文NO1 = tbl社内注文明細テーブル.注文NO AND 
           社内注文テーブル1.注文枝番1 = tbl社内注文明細テーブル.注文枝番 

left join prj_seisan.dbo.tbl注文書確定明細 on 社内注文テーブル1.注文NO1=prj_seisan.dbo.tbl注文書確定明細.指令書NO and 社内注文テーブル1.注文枝番1=prj_seisan.dbo.tbl注文書確定明細.指令書NO_枝番 and tbl社内注文明細テーブル.行番=prj_seisan.dbo.tbl注文書確定明細.行番

-- 2013/4/25 手配の購入依頼計追加
LEFT JOIN
(
select shirei.指令書NO,shirei.指令書NO_枝番,sum(shireimei.予定金額) as iraikei from prj_seisan.dbo.tblシステム部社内注文 as shirei 
INNER JOIN prj_seisan.dbo.tblシステム部社内注文明細 as shireimei 
on shirei.注文NO = shireimei.注文NO and shirei.注文枝番 = shireimei.注文枝番 
inner join -- データ選択
(
select 注文NO, max(注文枝番) as 注文枝番 from prj_seisan.dbo.tblシステム部社内注文 
group by 注文NO		
) as s2		
on  shirei.注文NO   =  s2.注文NO		
and shirei.注文枝番 =  s2.注文枝番		
WHERE  shirei.注文種別&lt;'3' and shirei.変更区分&lt;'2' GROUP BY shirei.指令書NO,shirei.指令書NO_枝番) as konyu 
ON tbl社内注文テーブル.注文NO = konyu.指令書NO and tbl社内注文テーブル.注文枝番 = konyu.指令書NO_枝番 

-- 2016/4/25 手配の出庫追加
LEFT JOIN -- データ選択
(
SELECT
 shi.指令書NO
 ,shi.指令書NO_枝番
 ,sum(shimei.予定金額) as 出庫依頼額
FROM prj_seisan.dbo.tblシステム部社内注文 shi
inner join prj_seisan.dbo.tblシステム部社内注文明細　shimei on shi.注文NO=shimei.注文NO　
and shi.注文枝番=shimei.注文枝番 
inner join -- データ選択
(
select 注文NO, max(注文枝番) as 注文枝番 from prj_seisan.dbo.tblシステム部社内注文 
group by 注文NO		
) as s2		
on  shi.注文NO   =  s2.注文NO		
and shi.注文枝番 =  s2.注文枝番		
where  shi.注文種別='3' and shi.変更区分&lt;'2' 
group by shi.指令書NO,shi.指令書NO_枝番
) shutu
ON tbl社内注文テーブル.注文NO = shutu.指令書NO and tbl社内注文テーブル.注文枝番 = shutu.指令書NO_枝番 

left JOIN  mst担当者マスタ ON
           tbl社内注文テーブル.生産部担当者コード = mst担当者マスタ.担当者コード

left JOIN tbl案件テーブル ON
           tbl社内注文テーブル.案件NO = tbl案件テーブル.案件NO

left JOIN prj_seisan.dbo.mst価格表 as kk ON 
           tbl社内注文明細テーブル.商品コード = kk.商品コード

WHERE           (tbl社内注文テーブル.依頼先部署コード =  @システム部署) AND    
                (tbl社内注文テーブル.仕切日 = ' ') 

AND tbl社内注文テーブル.生産部受付日 &gt; ' ' 
AND tbl社内注文テーブル.注文承認 &lt; 3 
AND tbl社内注文テーブル.依頼状況 &lt; 3 
AND tbl社内注文テーブル.変更区分 &lt; 2 
-- AND tbl社内注文テーブル.依頼先部署コード='FL' 
-- AND tbl社内注文テーブル.注文NO in ('CS1200245','DS1100365','GH1000103')
--　順序
order by   tbl社内注文テーブル.注文NO,tbl社内注文テーブル.注文枝番,tbl社内注文明細テーブル.行番</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
