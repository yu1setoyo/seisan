    <DataSet Name="DataSet3">
      <Query>
        <DataSourceName>DataSource2</DataSourceName>
        <CommandText>-------------------------------------
-- 出庫依頼予定額集計
-----
SELECT
--  tblシステム部社内注文.注文NO
--  ,tblシステム部社内注文.注文枝番
  tblシステム部社内注文.指令書NO
  ,tblシステム部社内注文.指令書NO_枝番
 ,sum(tblシステム部社内注文明細.予定金額) as 出庫依頼額
FROM
  tblシステム部社内注文
inner join tblシステム部社内注文明細　on tblシステム部社内注文.注文NO=tblシステム部社内注文明細.注文NO　
and tblシステム部社内注文.注文枝番=tblシステム部社内注文明細.注文枝番
where  tblシステム部社内注文.注文種別='3' 
and  tblシステム部社内注文.変更区分&lt;&gt;'2' 
and  tblシステム部社内注文.部署記号= 'FL'  ---@システム部署 
group by tblシステム部社内注文.指令書NO,tblシステム部社内注文.指令書NO_枝番</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="指令書NO">
          <DataField>指令書NO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="指令書NO_枝番">
          <DataField>指令書NO_枝番</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="出庫依頼額">
          <DataField>出庫依頼額</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet4">
      <Query>
        <DataSourceName>DataSource2</DataSourceName>
        <CommandText>-- システム受注残::購入依頼
-- and shirei.注文種別 &lt; '3'  --1:注文  2:購入  3:出庫 
--
SELECT
shi.指令書NO
,shi.指令書NO_枝番
,sum(shimei.予定金額) as 出庫依頼額
FROM prj_seisan.dbo.tblシステム部社内注文 shi
inner join prj_seisan.dbo.tblシステム部社内注文明細 shimei on shi.注文NO=shimei.注文NO 
and shi.注文枝番=shimei.注文枝番
where  shi.注文種別='2' 
and  shi.変更区分&lt;&gt;'2' 
and  shi.指令書NO='SH1700007'
-- tblシステム部社内注文.部署記号= 'FK'  ---@システム部署 
group by shi.指令書NO,shi.指令書NO_枝番</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="指令書NO">
          <DataField>指令書NO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="指令書NO_枝番">
          <DataField>指令書NO_枝番</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="出庫依頼額">
          <DataField>出庫依頼額</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
