  <DataSets>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>DataSource2</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@login部署">
            <Value>=Parameters!login部署.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  mst部署マスタ.部署記号
  ,mst部署マスタ.部署名称
  ,mst部署マスタ.ソート順
  ,mst部署マスタ.部署形態
FROM
  mst部署マスタ
where mst部署マスタ.部署形態 &gt;= '2' and mst部署マスタ.ソート順 &lt; 30
 AND mst部署マスタ.部署記号 = CASE WHEN @login部署 IN ('AP','AH','FH') THEN mst部署マスタ.部署記号
ELSE @login部署 END 


order by mst部署マスタ.ソート順</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="部署記号">
          <DataField>部署記号</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="部署名称">
          <DataField>部署名称</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ソート順">
          <DataField>ソート順</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="部署形態">
          <DataField>部署形態</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
