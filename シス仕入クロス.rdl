    <DataSet Name="DataSet9">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@仕切年月">
            <Value>=Parameters!仕切年月.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>with シス仕入合算 
AS
(
SELECT 
   tbl検収依頼明細.検収日 as 仕切日 
   ,left(tbl検収依頼明細.検収日,4)+'/'+substring(tbl検収依頼明細.検収日,6,2) as 仕切年月 
  ,tblシステム部社内注文.注文NO
  ,tblシステム部社内注文.注文枝番
  ,tblシステム部社内注文.依頼先部署コード
  ,tblシステム部社内注文.部署記号
  ,tblシステム部社内注文.指令書NO
  ,tblシステム部社内注文.指令書NO_枝番  
  ,tbl検収依頼明細.金額 as 確定金額
  ,'区分'='1'
FROM
  tblシステム部社内注文
inner join tbl検収依頼明細
on  tblシステム部社内注文.注文NO=tbl検収依頼明細.依頼NO and tblシステム部社内注文.注文枝番=tbl検収依頼明細.依頼NO_枝番 
union all
SELECT  
   tbl仕切明細.仕切受入日
  ,left(tbl仕切明細.仕切受入日,4)+'/'+substring(tbl仕切明細.仕切受入日,6,2) as 仕切年月 
  ,tbl仕切.指令書NO 
  ,tbl仕切.指令書NO_枝番 
  ,tbl仕切.部署記号 
  ,'依頼元'=CASE 
         WHEN left(tbl仕切.指令書NO,1) = 'L' THEN substring(tbl仕切.指令書NO,2,2)
         ELSE left(tbl仕切.指令書NO,2) END  
 
  ,tblシステム部社内注文.指令書NO as 指令NO
  ,tblシステム部社内注文.指令書NO_枝番 as 指令NO_枝番
  ,tbl仕切明細.生産仕切金額+tbl仕切明細.運送費 as 仕切額 
  ,'区分'='2'
FROM tbl仕切 
inner join tbl仕切明細 on tbl仕切.指令書NO=tbl仕切明細.指令書NO and tbl仕切.指令書NO_枝番=tbl仕切明細.指令書NO_枝番 
inner join tblシステム部社内注文 on tbl仕切.指令書NO=tblシステム部社内注文.注文NO and tbl仕切.指令書NO_枝番=tblシステム部社内注文.注文枝番
)
select 
   シス仕入合算.仕切日
  ,シス仕入合算.仕切年月 
  ,シス仕入合算.注文NO
  ,シス仕入合算.注文枝番
  ,シス仕入合算.依頼先部署コード
  ,シス仕入合算.部署記号
  ,シス仕入合算.指令書NO
  ,シス仕入合算.指令書NO_枝番
  ,シス仕入合算.確定金額 
  ,bsho.ソート順 
from シス仕入合算
inner join prj_shoden.dbo.mst部署マスタ as bsho on シス仕入合算.部署記号=bsho.部署記号 
where  -- シス仕入合算.部署記号=@システム部署 and 
仕切年月=datename("year",@仕切年月)+'/'+ datename("month",@仕切年月)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
