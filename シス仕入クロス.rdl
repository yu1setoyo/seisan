    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT
  tbl仕切.指令書NO 
  ,tbl仕切.指令書NO_枝番 
  ,tbl仕切明細.仕切受入日
  ,tbl仕切明細.生産仕切金額+tbl仕切明細.運送費 as 仕切額
 ,'依頼元'=CASE 
         WHEN left(tbl仕切.指令書NO,1) = 'L' THEN substring(tbl仕切.指令書NO,2,2)
         ELSE left(tbl仕切.指令書NO,2)
   END  
  ,tbl仕切.部署記号
FROM tbl仕切
inner join tbl仕切明細 on tbl仕切.指令書NO=tbl仕切明細.指令書NO and tbl仕切.指令書NO_枝番=tbl仕切明細.指令書NO_枝番 
where  tbl仕切明細.仕切受入日&gt;='2014/10/01' and tbl仕切明細.仕切受入日&lt;='2014/10/31'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
