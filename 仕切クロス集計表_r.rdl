        <CommandText>---------------------------------------
-- 仕切クロス集計　2013/01/22
---------------------------------------
with 仕切sysmm
 AS
 ( 
SELECT
   tbl仕切.指令書NO
  ,tbl仕切.指令書NO_枝番
  ,tbl仕切.部署記号
  ,'依頼元'=CASE 
         WHEN left(tbl仕切.指令書NO,1) = 'L' THEN substring(tbl仕切.指令書NO,2,2)
         ELSE left(tbl仕切.指令書NO,2)
   END  
-- ,tbl仕切.仕切完了日
  ,tbl仕切明細.仕切日
  ,tbl仕切明細.営業仕入金額
  ,tbl仕切明細.生産仕切金額
  ,tbl仕切明細.原価金額
  ,tbl仕切明細.運送費
  ,tbl仕切明細.仕切受入日
  ,bsho.部署形態                ----  eigyo
  ,bsho.ソート順                 ----- eigyo
  ,bsho2.部署形態 as 部署形態2  ----- sys   
  ,bsho2.ソート順 as ソート順2     ----- sys

FROM
  tbl仕切
  inner JOIN tbl仕切明細
  on tbl仕切.指令書NO=tbl仕切明細.指令書NO and tbl仕切.指令書NO_枝番=tbl仕切明細.指令書NO_枝番
  left JOIN prj_shoden.dbo.mst部署マスタ as bsho on
         CASE 
         WHEN left(tbl仕切.指令書NO,1) = 'L' THEN substring(tbl仕切.指令書NO,2,2)
         ELSE left(tbl仕切.指令書NO,2)
         END  = bsho.部署記号
  left JOIN prj_shoden.dbo.mst部署マスタ as bsho2 
    on tbl仕切.部署記号=bsho2.部署記号
----------
where 
 (bsho.部署形態='1' or bsho.部署記号='FH') 
 AND  tbl仕切明細.仕切受入日&gt;'' 
 and 
 CASE 
 WHEN @仕切年月 IS NULL then 1 -- @para入力がNULLなら判定しない。
  WHEN datename("year",tbl仕切明細.仕切日)+'/'+datename("month",tbl仕切明細.仕切日) =  
-- WHEN datename("year",tbl仕切.仕切完了日)+'/'+datename("month",tbl仕切.仕切完了日) =  
 datename("year",@仕切年月)+'/'+datename("month",@仕切年月) THEN 1 
 else 0 
 END &gt;0

UNION ALL 
SELECT 

mm.依頼NO 
,mm.依頼NO_枝番 
,'部署記号' = 'MM' 
,'依頼元' = CASE 
         WHEN left(mm.依頼NO,1) = 'L' THEN substring(mm.依頼NO,2,2)
         ELSE left(mm.依頼NO,2)
         END 
,mm.納入日    -- as 仕切日  
,mm.金額
,mm.金額 as 金額2 
,mm.金額 as 金額3 
,'金額4' = 0 
,mm.納入日 as dmy 
,bsho.部署形態 
,bsho.ソート順 
,bsho2.部署形態 as 部署形態2
,bsho2.ソート順  as ソート順2

from 
prj_shizai.dbo.発注納入履歴データ as mm 
left JOIN prj_shoden.dbo.mst部署マスタ as bsho on 
         CASE 
         WHEN left(mm.依頼NO,1) = 'L' THEN substring(mm.依頼NO,2,2)
         ELSE left(mm.依頼NO,2)       
         END  =bsho.部署記号
left JOIN prj_shoden.dbo.mst部署マスタ as bsho2 on 
         'MM'=bsho2.部署記号
----------
where
 (bsho.部署形態='1' or bsho.部署記号='FH') 
 and 
 CASE 
 WHEN @仕切年月 IS NULL then 1 
 WHEN datename("year",mm.納入日)+'/'+datename("month",mm.納入日) =  
 datename("year",@仕切年月)+'/'+datename("month",@仕切年月) THEN 1 
 else 0 
 END &gt;0
)
select * from 仕切sysmm 
-----------------------------------------------
-- Sum(Fields!営業仕入金額.Value+Fields!運送費.Value) 
-- Sum(Fields!生産仕切金額.Value+Fields!運送費.Value)
-----------------------------------------------</CommandText>
