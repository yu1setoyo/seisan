        </QueryParameters>
        <CommandText>SELECT
  tbl社内注文テーブル.注文NO
  ,tbl社内注文テーブル.注文枝番
  ,tbl社内注文テーブル.仕切日
  ,tbl社内注文テーブル.部署記号
  ,tbl社内注文テーブル.依頼先部署コード
  ,tbl社内注文テーブル.仕切状況
  ,tbl社内注文明細テーブル.行番
 ,tbl社内注文明細テーブル.確定金額 
,tbl社内注文明細テーブル.運送料 
,bsho_a.部署形態 
,bsho_a.ソート順 
,bsho_b.部署形態 as 部署形態_b 
,bsho_b.ソート順  as ソート順_b

FROM
  tbl社内注文テーブル
inner join tbl社内注文明細テーブル 
  on tbl社内注文テーブル.注文NO=tbl社内注文明細テーブル.注文NO and tbl社内注文テーブル.注文枝番=tbl社内注文明細テーブル.注文枝番 
inner join mst部署マスタ as bsho_a
  on tbl社内注文テーブル.部署記号=bsho_a.部署記号
inner join dbo.mst部署マスタ as bsho_b
  on tbl社内注文テーブル.依頼先部署コード=bsho_b.部署記号
where 
(bsho_a.部署形態='1' or bsho_a.部署記号='FH')  
 and
 (bsho_b.部署形態&gt;='2' or bsho_a.部署記号='FH')  
and
 case 
 WHEN @仕切年月 IS NULL then 1 -- @para入力がNULLなら判定しない。
 WHEN datename("year",tbl社内注文テーブル.仕切日)+'/'+datename("month",tbl社内注文テーブル.仕切日) =  
 datename("year",@仕切年月)+'/'+datename("month",@仕切年月) THEN 1 
 else 0 
 END &gt;0 
and tbl社内注文テーブル.仕切状況='3'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
