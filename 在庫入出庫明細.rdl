      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>-----------------------------------------
-- 在庫入出庫明細 2012/10/19~
-- 入出庫日は、chr。
-----------------------------------------

with 入出庫TBL合算
 AS
(
SELECT 
  tbl検収依頼明細.受入検査日 AS 入出庫日
  ,'入出庫区分' = 'N' 
  ,tbl検収依頼.依頼NO 
  ,tbl検収依頼明細.行番
  ,tbl検収依頼明細.商品コード AS 商品コード
  ,tbl検収依頼明細.商品名
  ,tbl検収依頼明細.規格仕様
  ,tbl検収依頼明細.数量 AS '入庫数'
  ,'出庫数' = 0
  ,依頼元部署コード AS 部署
  ,'指令書NO' = CASE 
                WHEN left(tbl検収依頼.指令書NO,9)='         ' then 'Lot発注' 
                else tbl検収依頼.指令書NO 
                END
  ,substring(tbl検収依頼明細.受入検査日,1,4)+'/'+substring(tbl検収依頼明細.受入検査日,6,2) as 入出庫年月
  ,tbl検収依頼明細.更新者ID 
  ,mstシステム部担当者.担当者名
  ,tbl検収依頼.更新日付
 
FROM tbl検収依頼  
INNER JOIN tbl検収依頼明細
 on tbl検収依頼.依頼NO=tbl検収依頼明細.依頼NO  AND tbl検収依頼.依頼NO_枝番=tbl検収依頼明細.依頼NO_枝番 and tbl検収依頼.検収依頼履歴NO=tbl検収依頼明細.検収依頼履歴NO
LEFT JOIN mstシステム部担当者
 on tbl検収依頼明細.更新者ID=mstシステム部担当者.担当者コード 
-- where tbl検収依頼明細.商品コード&lt;&gt;'9999999'
UNION ALL
SELECT 
  tbl実出庫.出庫日 AS 入出庫日
  ,'入出庫区分' = 'S' 
  ,tbl実出庫.依頼NO 
  ,tbl実出庫明細.行番
  ,tbl実出庫明細.商品コード AS 商品コード
  ,tbl実出庫明細.商品名
 ,tbl実出庫明細.規格仕様  AS 規格仕様
,'入庫数'= 0
  ,tbl実出庫明細.出庫数量 AS '出庫数' 
--
 ,'部署'=CASE 
         WHEN left(tbl実出庫.依頼NO,1) = 'L' THEN substring(tbl実出庫.依頼NO,2,2)
         ELSE left(tbl実出庫.依頼NO,2)
   END  
,'指令書NO' = CASE 
              WHEN left(tbl実出庫.指令書NO,9)='         ' then 'Lot出庫' 
              else tbl実出庫.指令書NO 
              END
-- ,tbl実出庫.指令書NO 
,substring(tbl実出庫.出庫日,1,4)+'/'+substring(tbl実出庫.出庫日,6,2) as 入出庫年月
,tbl実出庫.更新者ID 
,mstシステム部担当者.担当者名 
-- ,tbl実出庫.登録日付 
,tbl実出庫.更新日付 

FROM tbl実出庫  
INNER JOIN tbl実出庫明細
  on tbl実出庫.依頼NO=tbl実出庫明細.依頼NO and tbl実出庫.依頼NO_枝番=tbl実出庫明細.依頼NO_枝番 
  and tbl実出庫.実出庫履歴NO=tbl実出庫明細.実出庫履歴NO --2013/04/02修正
LEFT JOIN mstシステム部担当者
  on tbl実出庫.更新者ID=mstシステム部担当者.担当者コード 
where tbl実出庫明細.出庫数量&lt;&gt;0
UNION ALL
SELECT
  tbl入出庫.入出庫日  AS 入出庫日
  ,tbl入出庫.区分
  ,tbl入出庫.入出庫管理NO AS 依頼NO
  ,tbl入出庫明細.行番
  ,tbl入出庫明細.商品コード AS 商品コード
  ,tbl入出庫明細.商品名   
 ,tbl入出庫明細.規格仕様   AS 規格仕様
  ,'入庫数' = CASE 
              WHEN tbl入出庫.区分='N' then tbl入出庫明細.入出庫数量
              else 0
              END
  ,'出庫数' = CASE 
              WHEN tbl入出庫.区分='S' then tbl入出庫明細.入出庫数量
              else 0
              END
   ,tbl入出庫.部署記号
,'指令書NO' = '調整入力'
,substring(tbl入出庫.入出庫日,1,4)+'/'+substring(tbl入出庫.入出庫日,6,2) as 入出庫年月
,tbl入出庫.更新者ID 
,mstシステム部担当者.担当者名
,tbl入出庫.更新日付 

FROM
  tbl入出庫
  INNER JOIN tbl入出庫明細
    on tbl入出庫.入出庫管理NO=tbl入出庫明細.入出庫管理NO
  LEFT JOIN mstシステム部担当者
    on tbl入出庫.更新者ID=mstシステム部担当者.担当者コード
)
select * from 入出庫TBL合算
 where 入出庫TBL合算.部署=@部署 
 AND 入出庫TBL合算.商品コード&lt;&gt;'9999999'
 AND CASE
    WHEN len(@検索語句)=0 then 1                            -- len=0は全件対象。
    WHEN charindex(@検索語句,依頼NO+商品コード+規格仕様+指令書NO  COLLATE Japanese_CI_AS) &gt;0 then 1
    else 0
    END &gt; 0
 
 AND CASE 
 WHEN @入出庫年月 IS NULL then 1 
 WHEN 入出庫年月 = datename("year",@入出庫年月)+'/'+datename("month",@入出庫年月) THEN 1 
 else 0 
 END &gt;0 -- and 指令書NO='HS1200109'
 order by 入出庫TBL合算.入出庫日</CommandText>
        <QueryParameters>
          <QueryParameter Name="@入出庫年月">
            <Value>=Parameters!入出庫年月.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@部署">
            <Value>=Parameters!部署.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@検索語句">
            <Value>=Parameters!検索語句.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
    <DataSet Name="DataSet2">
      <Fields>
        <Field Name="部署記号">
          <DataField>部署記号</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ソート順">
          <DataField>ソート順</DataField>
