        </Field>
        <Field Name="部署名称">
          <DataField>部署名称</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="部署形態">
          <DataField>部署形態</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource2</DataSourceName>
        <CommandText>SELECT
  mst部署マスタ.部署記号
  ,mst部署マスタ.部署名称
  ,mst部署マスタ.ソート順
  ,mst部署マスタ.部署形態
FROM
  mst部署マスタ
where mst部署マスタ.部署形態 = '2' and mst部署マスタ.ソート順 &lt; 10
 AND mst部署マスタ.部署記号 = CASE WHEN @login部署 IN ('AP','AH','FH') THEN mst部署マスタ.部署記号
ELSE @login部署 END 


order by mst部署マスタ.ソート順</CommandText>
        <QueryParameters>
          <QueryParameter Name="@login部署">
            <Value>=Parameters!login部署.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
